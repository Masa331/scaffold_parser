<?xml version="1.0"?>
<xs:schema
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:cmn="common-types"
  xmlns:ord="order"
  xmlns="order"
  targetNamespace="order"
  elementFormDefault="qualified">

  <xs:import namespace="common-types" schemaLocation="spec/element_refs/common-types.xsd"/>

  <xs:complexType name="order">
    <xs:sequence>
      <xs:element name="name"/>
      <xs:element ref="cmn:item"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>
